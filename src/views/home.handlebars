<div>
    <h1>Listado de productos</h1>
    {{#each products.products}}
        <h6>{{this.title}}</h6>
        <p>{{this.category}}</p>
        <p>{{this.description}}</p>
        <p>{{this.price}}</p>
        <p>{{this.stock}}</p>
    {{/each}}
    <button class="logout">Logout</button>
    <script src="/files/logout.js"></script>
</div>
